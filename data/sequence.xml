<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for patient.sequence -->
        <record id="seq_hospital_patent" model="ir.sequence">
            <field name="name">Patient_Sequencer</field>
            <field name="code">hospital.patient.sequence</field>
            <field name="prefix">HP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hospital_appointment" model="ir.sequence">
            <field name="name">Appointment_Sequencer</field>
            <field name="code">appointment.sequence</field>
            <field name="prefix">AP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>


        <record id="seq_hospital_doctors" model="ir.sequence">
            <field name="name">Doctor_Sequencer</field>
            <field name="code">doctor.sequence</field>
            <field name="prefix">DOC</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
